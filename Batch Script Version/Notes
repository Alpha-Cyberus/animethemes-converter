for %f in (*.webm) do ffmpeg -i "%f" -vn -acodec "%~nf".ogg
for %v in (*.webm) do ffmpeg -i "%v" "%~nv.mp3"
for /R %v in (*.webm) do ffmpeg -i "%v" "%~nv.mp3"
FOR /R %v in (*.webm) DO ffmpeg -i "%~fv" "%~dpnv.mp3" & DEL /S "%~v"

----- split string into substrings based on delimiter
https://www.dostips.com/forum/viewtopic.php?f=3&t=6429
set _base=JujutsuKaisen-ED1-NCBD1080
set "_left=%_base:-=" & set "_right=%"
echo _

----- IDEA!!!
find the last substring (after the second -) then remove that from the whole!!
set a string, then immediately set it again but replacing a particular substring with something else which can easily be captured

----- HOW
set var=%somevar:*string1=string2%
will assign to var the value of somevar with all characters up to string1 replaced by string2. The enclosing quotes in a set command ensure that any stray trailing spaces on the line are not included in the value assigned.

----- SPLIT STRING and keep each part
SETLOCAL EnableDelayedExpansion
set i=1
set "x=monotonous"
set "x!i!=%x:o=" & set /A i+=1 & set "x!i!=%"
set x


----- Find postion of substring in string
set "s=This is the example."
call:findString s "the" pos

if not defined pos echo.Substring not found&goto:eof
echo.Position is: %pos%

PAUSE
goto:eof

:findString -- returns the zero based postion of one string in another string of maximum length of 1023 characters
::          -- %~1: in - varible name of a string to be serached
::          -- %~2: in - string to be found
::          -- %~3: out- return variable name, will be set to position or undefined if string not found
SETLOCAL ENABLEDELAYEDEXPANSION
set "pos="
set "str=!%~1!"
for /L %%a in (0,1,1023) do (
   set "s=!str:~%%a!"
   if not defined pos if "%~2!s:*%~2=!"=="!s!" set "pos=%%a"
)
ENDLOCAL & IF "%~3" NEQ "" SET "%~3=%pos%"
GOTO:EOF

----- Split string of unknown length, turn parts into other stuff
@echo off
setlocal EnableDelayedExpansion

set p=%%
set r1=ONE
set r2=TWO
set r3=THREE
set r4=FOUR
set r5=FIVE

set "x=3,1,5,4"
set "x2=%%r%x:,=!p!," & call set "x2=!x2!!p!r%%%"
set x


----- Remove leading and trailing whitespace
@echo off
setlocal EnableDelayedExpansion

set "x=     String   with   spaces     "

set "x=%x% "
set "i=0"
set "j="
set "w=%x: =" & (if not defined w (if not defined j (set /A i+=1) else set /A j+=1) else set j=1) & set "w=%"
set "x2=!x:~%i%,-%j%!"

echo "%x:~0,-1%"
echo "%x2%"
